{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "appName": {
      "value": "kursstatistik-api"
    },
    "env": {
      "value": "prod"
    },
    "secretNames": {
      "value": [
        "APPLICATIONINSIGHTS_CONNECTION_STRING",
        "CA_FILE",
        "KURSSTATISTIK_API_KEYS_0",
        "LADOK3_CERT",
        "LADOK3_CERT_KEY",
        "LADOK3_DATABASE",
        "LADOK3_PASSWORD",
        "LADOK3_USERNAME",
        "STUNNEL_HOST",
        "STUNNEL_CONFIGURATION_NAME",
        "STUNNEL_CONFIGURATION_CONNECT_URL",
        "STUNNEL_PORT"
      ]
    },
    "environmentVariables": {
      "value": {
        "LOGGING_ACCESS_LOG": "true",
        "LOGGING_CLIENT_LEVEL": "info",
        "LOGGING_LEVEL": "INFO",
        "NODE_ENV": "production",
        "SERVER_HOST_URL": "https://api.kth.se",
        "SERVER_PORT": "3000",
        "SERVICE_PUBLISH": "/api/kursstatistik",
        "STUNNEL_PORT": "11000"
      }
    },
    "monitorPath": {
      "value": "/api/kursstatistik/_monitor"
    },
    "websitePort": {
      "value": "3000"
    }
  }
}
